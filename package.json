{
  "name": "kiera-web",
  "version": "3.0.0",
  "private": true,
  "description": "Web portal for kiera-bot",
  "license": "MIT",
  "author": {
    "name": "Emma",
    "url": "https://github.com/rileyio/kiera-web"
  },
  "scripts": {
    "app:dev": "vue-cli-service serve --mode development",
    "app:build": "vue-cli-service build --mode production",
    "app:lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-cookies": "^1.7.0",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.7.0",
    "@types/axios": "^0.14.0",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.11.0",
    "css-loader": "^3.5.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mutationobserver-shim": "^0.3.3",
    "popper.js": "^1.16.0",
    "portal-vue": "^2.1.6",
    "prettier": "^2.0.4",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "typescript": "^3.8.3",
    "vue-cli-plugin-bootstrap-vue": "~0.6.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-explicit-any": 0
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 180,
    "trailingComma": "none",
    "tabWidth": 2,
    "useTabs": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
